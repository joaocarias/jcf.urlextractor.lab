{
    "manifest_version": 3,
    "name": "URL Fetcher",
    "version": "1.0",
    "permissions": ["activeTab", "tabs", "webRequest"],
    "background": {
      "service_worker": "background.js"
    },
    "host_permissions": ["<all_urls>"],
    "action": {
      "default_popup": "popup.html"      
    }
  }
  